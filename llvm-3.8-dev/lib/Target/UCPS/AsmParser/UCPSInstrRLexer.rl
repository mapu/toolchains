/*
 * replace the lexer generated by flex
 */

%%{
	machine UCPSInstrLexer;
	write data;
}%%

%%{ #// sub FSMs

main := |*
  'r'i ( '0' | ([1-9][0-9]*)) => {
		assert(atoi(ts+1)<32);
		return _RReg;
	};

*|;

}%%

