#define UBOOT_LOAD_BASE	0x10000
#define UBOOT_SIZE	0x70000
#define UBOOT_TEXT_BASE	0x60800000

int main(void)
{
	int i;
	for (i=0; i<UBOOT_SIZE; )
	{
		*((unsigned int*)(UBOOT_TEXT_BASE+i)) = *((unsigned int*)(UBOOT_LOAD_BASE+i));
		i += 4;
	}
	void (*e)() = (void(*)())0x60800000;
	e();
	return 0;
}
